function solution(name, yearning, photo) {
    var answer = [];
    
    
    for(let i = 0; i < photo.length; i++) {
        //초기값 설정
        answer[i] = 0;
        
        for(let j = 0; j < photo[i].length; j++) {
            for(let k = 0; k < name.length; k++) {
                if(photo[i][j] === name[k]) {
                    answer[i] += yearning[k];
                }
            }
        }
    }
    
    return answer;
}
