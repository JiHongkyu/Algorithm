function solution(s) {
    let stack = [];
    
    for(let i = 0; i < s.length; i++) {
        //stack에 문자 하나씩 추가
        stack.push(s[i]);
        //stack의 마지막값과 마지막 전값을 비교 후 같으면 둘 다 삭제
        if(stack[stack.length-1] === stack[stack.length-2]) {
            stack.pop();
            stack.pop();
        }
    }
    
    //stack의 길이가 0이면 1리턴 0이 아니면 0리턴
    return stack.length === 0 ? 1 : 0;
}
