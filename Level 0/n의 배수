// 방법 1

function solution(n, numlist) {
    var answer = [];
    
    for(let i = 0; i < numlist.length; i++) {
        if(numlist[i] % n === 0) answer.push(numlist[i]);
    }
    return answer;
}

// 1. n으로 나누어지면 배열에 추가

// 방법 2
function solution(n, numlist) {
    return numlist.filter( (value) => value % n === 0);
}

// 1. filter함수를 사용하여 n으로 나누었을 때 0이 되는 값만 골라내서 저장
