function solution(num_list, n) {
    var answer = [];
    var k = 0;
    
    for(let i = 0; i < num_list.length/n; i++) {
        var cells = [];
        
        for(let j = 0; j < n; j++) {
            cells.push(num_list[k]);
            k++;
        }
        answer.push(cells);
    }
    return answer;
}
