//방법 1
function solution(numbers, num1, num2) {
    var answer = [];
    
    for(let i = num1; i <= num2; i++) {
        answer.push(numbers[i]);
    }
    return answer;
}

// 1. num1부터 num2까지 돌면서 새 배열에 추가

//방법 2 (함수 사용)
function solution(numbers, num1, num2) {
    return numbers.slice(num1, num2+1);
}
